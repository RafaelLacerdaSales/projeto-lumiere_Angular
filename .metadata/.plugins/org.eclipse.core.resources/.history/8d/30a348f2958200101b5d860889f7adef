package com.lumiere.project.repositories;
import java.util.List;
import java.util.Optional;

import org.springframework.data.jpa.repository.JpaRepository;
import com.lumiere.project.entities.CadastroEntities;
public interface UsersRepositories extends JpaRepository<CadastroEntities, Long>{
   Optional<CadastroEntities> findByEmail(String email);
   List<CadastroEntities> findByNome(String nome);
   List<CadastroEntities> findBySenha(String senha);
   List<CadastroEntities> findByCpf(String cpf);
   List<CadastroEntities> findByTelefone(String telefone);
}