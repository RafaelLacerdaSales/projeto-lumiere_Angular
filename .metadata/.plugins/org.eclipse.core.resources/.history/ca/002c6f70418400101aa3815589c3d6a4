package com.lumiere.project.entities;

import jakarta.validation.constraints.NotBlank;


public class LoginDTO {
	@NotBlank
	private String email;
	@NotBlank
	private String Senha;
	
	private boolean loginValidar;

	public LoginDTO() {}

	
	public LoginDTO(@NotBlank String email, @NotBlank String senha, boolean loginValidar) {
		super();
		this.email = email;
		Senha = senha;
		this.loginValidar = loginValidar;
	}

	

	public boolean isLoginValidar() {
		return loginValidar;
	}


	public void setLoginValidar(boolean loginValidar) {
		this.loginValidar = loginValidar;
	}


	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getSenha() {
		return Senha;
	}

	public void setSenha(String senha) {
		Senha = senha;
	}
	
	
}
